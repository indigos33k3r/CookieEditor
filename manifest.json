{
  "name": "CookieEditor",
  "description": "Editor website cookie",
  "version": "0.1",
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Click to show popup",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
